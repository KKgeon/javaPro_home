package days11;

import java.util.Arrays;

public class Ex06 {
	public static void main(String[] args) {
		int [] m = { 3, 92, 3, 40, 71, 91, 61, 92, 76, 71, 59, 54, 64, 48, 66, 92, 25, 20, 73, 37 };
		//max, min, sum, avg
		
		for (int i = 0; i <= m.length; i++) {
			int sum=0;
			sum = m[i] + sum;
		}
		System.out.println(Arrays.toString(m));
	}//m
}//c
