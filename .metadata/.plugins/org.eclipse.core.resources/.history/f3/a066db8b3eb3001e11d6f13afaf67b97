package javaPro;

import java.util.Scanner;

public class Sample4 {
	static int [] n = new int [3];
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		appendArray(n);
		dispArray();

	}//m

	private static void dispArray() {
		System.out.println();
	}

	private static void appendArray(int [] n) {
		int index = 0;
		Scanner sc = new Scanner(System.in);
		int m = sc.nextInt();
		
		do {
			addArr();
		} while (n.length == index);
		n[index++] = m;
		
		
		
		
	}

	private static void addArr() {
		int [] temp = new int [n.length + 3];

		for (int i = 0; i < n.length; i++) {
			temp[i] = n[i];
		}
		n = temp;
	}

}//c
