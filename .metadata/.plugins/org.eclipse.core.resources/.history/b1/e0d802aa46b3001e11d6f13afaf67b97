package days11;

public class Ex03 {
	public static void main(String[] args) {
		int [] m =  {3,5,2,4,1};
		int foundIndex = indexOf( m, 400 );
System.out.println(foundIndex);		
		
	}//m

	private static int indexOf(int[] m, int n) {
		for (int i = 0; i < m.length; i++) {
			if (m[i] == n) return i;
		}
		return -1;
	}
}//c
